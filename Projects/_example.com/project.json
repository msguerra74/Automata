{
  "name": "_example.com",

  "site": {
    "url": "http://localhost:4000",
    "title": "Website Title",
    "description": "Website description.",
    "owner": "Owner Name",
    "email": "owner@email.com",
    "google_analytics_id": "UA-xxxxxxxx-x"
  },

  "jekyll": {
    "exclude": "['.htaccess']",
    "future": "false",
    "include": "[]",
    "keep_files": "['.git', 'assets']",
    "lsi": "false",
    "markdown": "rdiscount",
    "paginate": "7",
    "permalink": "/:categories/:title/",
    "show_drafts": "false",
    "timezone": ""
  },

  "dev": {
    "name": "Developer Name",
    "twitter": "@twitter_handle",
    "website": "http://developer-website.com",
    "banner": "Copyright <%= current_year %>, <%= prj.dev.name %> | <%= prj.dev.twitter %> | <%= prj.dev.website %>",
    "url": "http://localhost:<%= prj.dev.port %>",
    "port": "4000"
  }
}